    "vue-i18n": "^8.0.0",
    "vue-router": "^3.0.3",
    "vuetify": "^1.5.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@mdi/font": "^3.8.95",
    "@vue/cli-plugin-babel": "^3.9.0",
    "@vue/cli-plugin-eslint": "^3.9.0",
    "@vue/cli-service": "^3.9.0",
    "axios": "^0.19.0",
    "babel-eslint": "^10.0.1",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^5.16.0",
